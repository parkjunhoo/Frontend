<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>serverweb</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>basic.FirstServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/first.sds</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>gugu</servlet-name>
		<servlet-class>basic.SecondServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gugu</servlet-name>
		<url-pattern>/gugudan.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>third</servlet-name>
		<servlet-class>basic.ThirdServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>third</servlet-name>
		<url-pattern>/third.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>basic.DBServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>life</servlet-name>
		<servlet-class>basic.LifeCycleTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>life</servlet-name>
		<url-pattern>/life.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>param</servlet-name>
		<servlet-class>basic.ParamTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>param</servlet-name>
		<url-pattern>/paramtest</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>exam</servlet-name>
		<servlet-class>basic.ExamServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>exam</servlet-name>
		<url-pattern>/exam</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>calc</servlet-name>
		<servlet-class>basic.CalcServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>calc</servlet-name>
		<url-pattern>/calc.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>user_login</servlet-name>
		<servlet-class>basic.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>user_login</servlet-name>
		<url-pattern>/user/login.do</url-pattern>
	</servlet-mapping>

</web-app>