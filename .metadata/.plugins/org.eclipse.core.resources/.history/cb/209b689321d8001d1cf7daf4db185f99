<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
		<script type="text/javascript">
			function printGugu(){
				document.write("<table border='1'>");
				for(i=1;i<=9;i++){
					document.write("<tr>");
					document.write("<td><h3>"+dan+"*"+i+"="+(dan*i)+"</h3></td>");
					document.write("</tr>");
				}	
				document.write("</table>");
			}
			
			function getSum(num1,num2){
				sum = 0;
				for(i=num1; i<=num2; i++){
					sum += i;
				}
				return sum;
			}
			
			function calc(num1,num2,opr){
				switch(opr){
					case "+":
						return num1+num2;
					case "-":
						return num1-num2;
					case "*":
						return num1*num2;
					case "/":
						return num1/num2;
					default:
						return null;
				}
			}
		</script>
	</head>
	<body>
		<script type="text/javascript">
			printGugu();
			getSum(1,10);
			document.write("<br><h1>"+calc(1,1,"+")+"</h1>");
		</script>
	</body>
</html>