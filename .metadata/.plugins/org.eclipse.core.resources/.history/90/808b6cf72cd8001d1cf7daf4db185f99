		//=>선언된 위치 (정의된 블럭) 에서만 사용할 수 있다.
			var tt = "t";
			const data = 100;
			let point = 99;
			let myval = 0;
			if(point > 90){
				let ifval = "test";
				myval = 2000;
				console.log("if블럭에서 정의된 val=>"+ifval);
			}
			console.log("point=>"+point);
			console.log("myval(head)=>"+myval);
			
		</script>
	</head>
