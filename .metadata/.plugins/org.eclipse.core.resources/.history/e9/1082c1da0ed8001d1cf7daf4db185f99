<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
	</head>
	<body>
		<script type="text/javascript">
			var html = "<table border='1'>";
			for(x=1; x<=9; x++){
				html += "<tr>";
				for(y=1; y<=9; y++){
					if(x==y){
						html += "<td style='backgroundColor:yellow;'>"+x+"*"+y+"="+(x*y)+"</td>";
					}else{
						html += "<td>"+x+"*"+y+"="+(x*y)+"</td>";
					}
					
				}
				html += "</tr>";
			}
			html += "</table>";
			
			document.write(html);
		</script>
	</body>
</html>